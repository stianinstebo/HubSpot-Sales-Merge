<script>
	function onSignIn(googleUser) {
		var user = sessionStorage.getItem("email");
		console.log("storage user: " + user);

	  	var profile = googleUser.getBasicProfile();

		$("#g-name").text(profile.getName());
		$("#g-name").show();
		
		$("#google-btn").hide();

		$("#g-user-img").attr('src', profile.getImageUrl());
		$("#g-user-img").show();
		
		if (profile.getEmail().includes('@synlighet')) {
			loadData();
		} else {
			$("#errormessage").text("Kun interne kontoer har tilgang");
			$("#refreshLink").show();
			signOut();
		}
	}

	function signOut() {
	    var auth2 = gapi.auth2.getAuthInstance();
	    auth2.signOut().then(function () {
	      console.log('User signed out.');
	    });
	  }
</script>